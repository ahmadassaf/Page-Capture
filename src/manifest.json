{
  "name": "Page Capture (by Yieldbot)",
  "description": "A Chrome Extension that allows page capturing from within your UI Application.",
  "version": "0.0.2",
  "manifest_version": 2,
  "author": "Andrei McMillan <amcmillan@yieldbot.com>",
  "icons": {
    "32": "logo.png",
    "48": "logo.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "js": [
        "content.script.index.js"
      ],
      "css": []
    }
  ],
  "web_accessible_resources": [
    "content.script.api.js"
  ]
}